        "eslint": "5.9.0",
        "eslint-plugin-jest": "22.1.0",
        "jest": "23.6.0"
    },
    "dependencies": {
        "chalk": "^2.4.1",
        "dotenv": "6.1.0",
        "node-fetch": "2.3.0"
